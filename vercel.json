{
  "version": 2,
  "builds": [
    { "src": "api_build/server/app.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist/public" } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api_build/server/app.js" },
    { "handle": "filesystem" },
    { "src": "(.*)", "dest": "/index.html" }
  ]
}
